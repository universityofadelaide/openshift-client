{
  "apiVersion": "networking.k8s.io/v1",
  "kind": "NetworkPolicy",
  "metadata": {
    "name": "test-np"
  },
  "spec": {
    "ingress": [
      {
        "from": [
          {
            "podSelector": {
              "matchLabels": {
                "app": "node-19"
              }
            }
          }
        ],
        "ports": [
          {
            "port": 11312,
            "protocol": "TCP"
          }
        ]
      }
    ],
    "podSelector": {
      "matchLabels": {
        "application": "datagrid-app"
      }
    },
    "policyTypes": [
      "Ingress"
    ]
  }
}
