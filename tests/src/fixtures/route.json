{
  "apiVersion": "route.openshift.io/v1",
  "kind": "Route",
  "metadata": {
    "name": "route-test",
    "labels": {
      "testing-label": "label-testing"
    },
    "annotations": {
      "testing-annotation": "annotation-testing"
    }
  },
  "spec": {
    "host": "route.host",
    "path": "/route/path",
    "tls": {
      "insecureEdgeTerminationPolicy": "Allow",
      "termination": "edge"
    },
    "to": {
      "kind": "Service",
      "name": "svc-test",
      "weight": 50
    },
    "wildcardPolicy": "None"
  }
}
