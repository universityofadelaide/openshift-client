{
    "name": "universityofadelaide/openshift-client",
    "description": "Simple OpenShift client for PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Barton",
            "email": "c.barton@adelaide.edu.au"
        },
        {
            "name": "Casey Fulton",
            "email": "casey.fulton@adelaide.edu.au"
        }
    ],
    "require": {
        "guzzlehttp/guzzle" : "^6.5.5",
        "symfony/serializer": "~4.4",
        "symfony/phpunit-bridge": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~9.5",
        "mockery/mockery" : "~1.5",
        "php-coveralls/php-coveralls": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "UniversityOfAdelaide\\OpenShift\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "UniversityOfAdelaide\\OpenShift\\Tests\\": "tests/src"
      }
    }
}
